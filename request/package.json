{
	"name": "miniprogram-request",
	"version": "3.2.0",
	"description": "A better Request package for MiniProgram API",
	"keywords": [
		"miniprogram",
		"wx.request"
	],
	"homepage": "https://miniprogram-network.newfuture.cc/request/",
	"bugs": {
		"url": "https://github.com/NewFuture/miniprogram-network/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/NewFuture/miniprogram-network.git"
	},
	"license": "Apache-2.0",
	"author": "NewFuture",
	"files": [
		"dist/**",
		"types/**",
		"src/**"
	],
	"main": "dist/index.js",
	"module": "es/index.js",
	"types": "types/index.d.ts",
	"scripts": {
		"build": "npm run build:es5 && npm run build:es6",
		"build:es5": "tsc --declarationDir types",
		"build:es6": "tsc -m es6 --outDir es",
		"clean": "rm -rf es dist types ",
		"lint": "tslint -p . -c ../tslint.json",
		"lint:fix": "tslint --fix -p . -c ../tslint.json",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"miniprogram-network-life-cycle": "^3.2.0",
		"miniprogram-network-utils": "^3.2.0",
		"miniprogram-queue": "^3.2.0"
	}
}
